/ {
    aliases {
        sw0 = &greenbtn;
    };

    
    gpio_keys {
        compatible = "gpio-keys";
        greenbtn: green_btn {
            gpios = <&gpio0 3 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
            label = "Green button on outside of case, hooked to pin D7";
        };
    };

};

&i2c1 {
    status = "okay";
    bme280@76 {
        compatible = "bosch,bme280";
        reg = <0x76>;
        label = "BME280_I2C";
    };

};